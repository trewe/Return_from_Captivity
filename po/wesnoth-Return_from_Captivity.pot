#. [message]: speaker=unit
#: ./scenarios/01_The_Prison.cfg:384
msgid "The wall is weak here! I'll try to break through!"
msgstr ""

#. [response]: speaker=second_unit
#: ./scenarios/01_The_Prison.cfg:388
msgid "Thank you for making me free. In gratitude I'll join you, $unit.name, in the battle."
msgstr ""

#. [message]: speaker=unit
#: ./scenarios/01_The_Prison.cfg:392
msgid "I can pass through here!"
msgstr ""

#. [response]: speaker=second_unit
#: ./scenarios/01_The_Prison.cfg:396
msgid "I'm free at last! I'll help you $unit.name to get away from here!"
msgstr ""

#. [message]: speaker=unit
#: ./scenarios/01_The_Prison.cfg:400
msgid "I can smash the wall here!"
msgstr ""

#. [response]: speaker=second_unit
#: ./scenarios/01_The_Prison.cfg:404
msgid "Thanks for rescuing me! I will serve you, $unit.name till you get home!"
msgstr ""

#. [message]: speaker=unit
#: ./scenarios/01_The_Prison.cfg:408
msgid "The wall collapsed!"
msgstr ""

#. [response]: speaker=second_unit
#: ./scenarios/01_The_Prison.cfg:412
